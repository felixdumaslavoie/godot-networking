[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 = "res://../rust/target/debug/libhot_reload.so"
linux.release.x86_64 = "res://../rust/target/release/libhot_reload.so"
windows.debug.x86_64 = "res://../rust/target/debug/hot_reload.dll"
windows.release.x86_64 = "res://../rust/target/release/hot_reload.dll"
